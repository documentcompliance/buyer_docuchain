{
	"welcomeFile": "/compliance-ui/index.html",
	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/compliance-ui/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "^/docuchain/(.*)$",
		"target": "$1",
		"authenticationType": "none",
		"destination": "buyer_docuchain",
		"csrfProtection": false
	}]
}